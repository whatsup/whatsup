{
    "name": "@whatsup/webpack-loader-css-components",
    "description": "Webpack css loader for whatsup application",
    "version": "2.2.17",
    "author": "Denis Churbanov",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/whatsup/whatsup.git",
        "directory": "packages/webpack-loader-css-components"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./factory": {
            "types": "./dist/factory.d.ts",
            "default": "./dist/factory.js"
        }
    },
    "scripts": {
        "start": "rollup -c -w",
        "build": "rollup -c && tsc -p ./tsconfig.build.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@whatsup/jsx": "^2.2.17",
        "cosmiconfig": "^7.0.1",
        "postcss": "^8.4.14",
        "postcss-import": "^14.1.0",
        "postcss-modules": "^4.3.1",
        "postcss-url": "^10.1.3"
    },
    "devDependencies": {
        "@types/postcss-import": "^14.0.0",
        "@types/postcss-url": "^10.0.0"
    }
}
