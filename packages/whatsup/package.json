{
    "name": "whatsup",
    "description": "A frontend framework for chillout-mode development ðŸ¥¤",
    "version": "2.3.11",
    "author": "Denis Churbanov",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/whatsup/whatsup.git",
        "directory": "packages/whatsup"
    },
    "scripts": {
        "build": "rollup -c && tsc -p ./tsconfig.build.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": "./dist/core.js",
        "./jsx": "./dist/jsx.js",
        "./cssx": "./dist/cssx.js",
        "./equals": "./dist/equals.js",
        "./route": "./dist/route.js"
    },
    "typings": "dist/core.d.ts",
    "typesVersions": {
        "*": {
            "jsx": [
                "./dist/jsx.d.ts"
            ],
            "cssx": [
                "./dist/cssx.d.ts"
            ],
            "equals": [
                "./dist/equals.d.ts"
            ],
            "route": [
                "./dist/route.d.ts"
            ]
        }
    },
    "dependencies": {
        "@whatsup/core": "^2.3.8",
        "@whatsup/cssx": "^2.3.11",
        "@whatsup/equals": "^2.3.8",
        "@whatsup/jsx": "^2.3.11",
        "@whatsup/route": "^2.3.11"
    }
}
