{
    "name": "whatsup",
    "description": "A frontend framework for chillout-mode development ðŸ¥¤",
    "version": "2.2.19",
    "author": "Denis Churbanov",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/whatsup/whatsup.git",
        "directory": "packages/whatsup"
    },
    "scripts": {
        "build": "rollup -c && tsc -p ./tsconfig.build.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "types": "./dist/core.d.ts",
            "default": "./dist/core.js"
        },
        "./jsx": {
            "types": "./dist/jsx.d.ts",
            "default": "./dist/jsx.js"
        },
        "./route": {
            "types": "./dist/route.d.ts",
            "default": "./dist/route.js"
        }
    },
    "dependencies": {
        "@whatsup/core": "^2.2.19",
        "@whatsup/jsx": "^2.2.19",
        "@whatsup/route": "^2.2.19"
    }
}
